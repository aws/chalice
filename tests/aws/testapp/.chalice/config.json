    "dev": {
      "api_gateway_stage": "api",
      "custom_domain_name": {
        "name": "api.example.com",
        "security_policy": "TLS 1.2|TLS 1.0",
        "endpoint_configuration": "Edge Optimized|Regional",
        "acm_certificate": "arn:aws:acm:example",
